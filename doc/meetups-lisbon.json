"{""id"":256,""name"":""Encontros Liberais Sociais de Lisboa"",""description"":""<p>Encontre-se com outros liberais em Lisboa e discuta os temas quentes da pol&iacute;tica nacional e internacional.</p>"",""link"":""http://www.meetup.com/liberalismo-lisboa/"",""start_date"":null,""end_date"":null,""created_at"":""2015-09-01T23:11:59.318577"",""updated_at"":""2015-09-01T23:11:59.318577"",""external_id"":""857875"",""external_image"":""http://photos2.meetupstatic.com/photos/event/8/3/b/b/thumb_19113723.jpeg"",""source_site"":1,""disabled"":false}"
